{
  "name": "react-template",
  "private": true,
  "version": "0.1.0",
  "description": "React + TS + Vite + Less + Antd6 template",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@latest",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@ant-design/icons": "latest",
    "ahooks": "latest",
    "antd": "latest",
    "axios": "latest",
    "react": "latest",
    "react-activation": "latest",
    "react-dom": "latest",
    "react-router-dom": "latest",
    "zustand": "latest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "jsdom": "latest",
    "less": "latest",
    "prettier": "latest",
    "typescript": "latest",
    "vite": "8.0.0-beta.15",
    "vitest": "latest"
  }
}
